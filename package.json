{
	"name": "typescript-djuli",
	"version": "1.0.0",
	"description": "",
	"main": "./src/bin/www.ts",
	"scripts": {
		"test": "jest",
		"lint": "eslint */**/*.{js,ts,tsx} --quiet --fix",
		"prepare": "husky install",
		"clean": "rimraf ./build/* && rimraf ./logs/*",
		"build": "npm run clean && npm run lint && webpack --watch",
		"start:dev": "SET NODE_ENV=development & npm run build",
		"run:dev": "pm2 start ./config/pm2/pm2-dev-process.config.js",
		"start:prod": "SET NODE_ENV=production & npm run build",
		"run:prod": "pm2 start ./config/pm2/pm2-prod-process.config.js",
		"monitor": "pm2 monit",
		"logs": "pm2 logs api-rest --lines 2000",
		"stop": "pm2 delete api-rest && npm run clean"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/jest": "^28.1.6",
		"@types/module-alias": "^2.0.1",
		"@types/node": "^18.0.6",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-n": "^15.2.4",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-standard": "^5.0.0",
		"husky": "^8.0.0",
		"jest": "^24.9.0",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"supertest": "^6.2.4",
		"ts-jest": "^28.0.7",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"webpack-node-externals": "^3.0.0",
		"webpack-shell-plugin": "^0.5.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"eslint --fix"
		]
	},
	"dependencies": {
		"@types/body-parser": "^1.19.2",
		"@types/webpack": "^5.28.0",
		"body-parser": "^1.20.0",
		"express": "^4.18.1",
		"fork-ts-checker-webpack-plugin": "^7.2.13",
		"module-alias": "^2.2.2",
		"pm2": "^5.2.0",
		"winston": "^3.8.1"
	},
	"_moduleAliases": {
		"@root": ".",
		"@config": "src/config"
	}
}
